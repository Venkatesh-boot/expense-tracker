{
  "name": "springboot-app",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "root": "springboot-app",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd springboot-app && ./mvnw clean package"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd springboot-app && ./mvnw spring-boot:run"
      }
    }
  }
}
